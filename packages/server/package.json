{
    "private": true,
    "name": "@primodiumxyz/octane-server",
    "version": "0.0.1",
    "description": "Octane server implementation - Fork of @solana/octane-server with improvements",
    "repository": "https://github.com/primodiumxyz/octane",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
