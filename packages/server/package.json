        "commander": "^9.4.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "express-rate-limit": "^6.4.0",
        "next": "^12.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^12.2.3",
        "@types/bs58": "^4.0.1",
        "@types/cache-manager": "^3.4.2",
        "@types/commander": "^2.12.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/react": "^18.0.15",
        "dotenv": "^16.0.1",
        "dotenv-cli": "^6.0.0",
